{
	"name": "josefglatz/cropvariantsbuilder",
	"type": "typo3-cms-extension",
	"description": "Simplify writing cropVariants for TYPO3 Integrators/Developers",
	"keywords": [
		"TYPO3",
		"extension",
		"cropvariant",
		"cropvariants",
		"imagemanipulation"
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^11.5 || ^12.4 || ^13.4",
		"php": ">=8.0.0"
	},
	"replace": {
		"typo3-ter/cropvariantsbuilder": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JosefGlatz\\CropVariantsBuilder\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "cropvariantsbuilder"
		}
	}
}
